source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '7.0'

inhibit_all_warnings!

target :victorious do
    pod 'RestKit', '~> 0.22.0'
    pod 'IGDigest', '~> 1.1.0'
    pod 'TestFlightSDK', '~> 3.0.0'
    pod 'TTTAttributedLabel', '~>1.8.1'
    pod 'MBProgressHUD', '~> 0.8'
    pod 'FlurrySDK', '~> 5.4'
    pod 'CCHLinkTextView'
    link_with 'victorious'

    target :test do
        pod 'Nocilla', :git => 'https://github.com/TouchFrame/Nocilla.git', :branch => 'block-response'
        link_with 'victoriousTests'
    end
end
